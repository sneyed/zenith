  "author": "sneyed <sneyed@gmail.com>",
  "license": "UNLICENSED",
  "description": "Personal component library",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sneyed/zenith.git"
  },
  "bugs": {
    "url": "https://github.com/sneyed/zenith/issues"
  },
  "homepage": "https://github.com/sneyed/zenith#readme",
  "keywords": [
    "react"
  ],
  "scripts": {
    "bundle": "microbundle --no-compress --format modern,cjs --jsx React.createElement",
    "bundlew": "microbundle watch --no-compress --format modern,cjs --jsx React.createElement",
    "pretty": "prettier --write **/*.js",
    "prepare": "node src/lib/nopublish",
    "deploy": "rm -rf ./dist && npm run bundle && np",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "babel-loader": "^8.2.2",
    "core-js": "^3.6.5",
    "husky": "^4.2.5",
    "microbundle": "^0.12.4",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.32.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
